<PropertyList include="../c172r/c172r-jsbsim-base.xml">

 <sim>

  <description>Cessna 172 with a full screen, hi-res panel</description>
 
  <panel>
   <path>Aircraft/c172/Panels/c172-610x-panel.xml</path>
